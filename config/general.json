{
    "local_store": "store.json",
    "date_format": {
        "no_year": "%-m/%-d",
        "with_year": "%-m/%-d/%y"
    },
    "header_format": "---=== {} ===---",
    "relative_days": 7,
    "default_tags": ["focus", "next"],
    "fuzzy_search": {
        "weights": {
            "name": 10,
            "notes": 3,
            "tag_exact": 4,
            "tag_partial": 1
        }
    }
}